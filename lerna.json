{
  "lerna": "4.0.0",
  "version": "0.2.1",
  "useWorkspaces": true,
  "npmClient": "yarn",
  "command": {
    "run": {
      "stream": true
    },
    "publish": {
      "forcePublish": true,
      "skipGit": true,
      "registry": "http://r.npm.sankuai.com",
      "ignoreChanges": "",
      "message": "chore(release): publish %s"
    },
    "version": {
      "message": "chore(version): change version %s"
    }
  },
  "packages": [
    "packages/l10n-web"
  ]
}
